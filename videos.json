# Ouvrir index.html et ajouter les vidéos dans le contenu
with open('index.html', 'r') as file:
    html_content = file.read()

# Ajouter les vidéos récupérées à la fin du fichier HTML
for video in videos:
    iframe_html = f'<div class="poster" style="margin: 20px auto; max-width: 560px; text-align: center;">\n' \
                  f'  <iframe src="{video["url"]}" width="100%" height="315" style="border: none; border-radius: 5px;" allowfullscreen></iframe>\n' \
                  f'</div>\n'
    html_content += iframe_html

# Sauvegarder le fichier index.html mis à jour
with open('index.html', 'w') as file:
    file.write(html_content)

print("Le fichier index.html a été mis à jour avec les nouvelles vidéos.")


